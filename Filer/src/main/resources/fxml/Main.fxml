                  <Separator orientation="VERTICAL" />
                  <Button mnemonicParsing="false" text="Cut" />
                  <Button mnemonicParsing="false" text="Copy" />
                  <Button mnemonicParsing="false" text="Paste" />
                  <Button mnemonicParsing="false" text="Shortcut" />
                  <Separator orientation="VERTICAL" />
                  <Button mnemonicParsing="false" text="Select all" />
                  <Button mnemonicParsing="false" text="Select change" />
                  <Separator orientation="VERTICAL" />
                  <Separator orientation="VERTICAL" />
                  <Separator orientation="VERTICAL" />
              </items>
            </ToolBar>
            <GridPane hgap="2.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" />
